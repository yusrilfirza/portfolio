{
  "name": "my-porto",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "core-util-is": "^1.0.2",
    "gh-pages": "^3.2.3",
    "postcss-cli": "^7.1.2",
    "postcss-import": "^12.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-modal-gallery": "^1.1.3",
    "react-scripts": "4.0.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "watch": "^1.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "build:tailwind": "postcss src/styles.css -o src/styles.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles.output.css -c 'npm run build:tailwind'",
    "start:react": "react-scripts start",
    "prebuild": "NODE_ENV=production run-s build:tailwind",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "homepage": "https://yusrilfirza.github.io/portfolio/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.4",
    "autoprefixer": "^9.8.6",
    "chokidar-cli": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.36",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.4"
  }
}
